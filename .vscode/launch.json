  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Debug server",
      "port": 9229,
      "address": "0.0.0.0",
      "remoteRoot": "/usr/dev",
      "localRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "/usr/dev/*": "${workspaceRoot}/*"
      }
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Debug users",
      "port": 9231,
      "address": "0.0.0.0",
      "remoteRoot": "/usr/dev",
      "localRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "/usr/dev/*": "${workspaceRoot}/*"
      }
    }
  ]
}
