{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "packages",
  "projects": {
    "zauth.angular.client": {
      "root": "packages/zauth.angular.client",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "z",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss",
          "spec": false
        },
        "@schematics/angular:class": {
          "spec": false
        },
        "@schematics/angular:directive": {
          "spec": false
        },
        "@schematics/angular:guard": {
          "spec": false
        },
        "@schematics/angular:module": {
          "spec": false
        },
        "@schematics/angular:pipe": {
          "spec": false
        },
        "@schematics/angular:service": {
          "spec": false
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "packages/zauth.angular.client/dist",
            "polyfills": "packages/zauth.angular.client/src/polyfills.ts",
            "index": "packages/zauth.angular.client/src/index.html",
            "main": "packages/zauth.angular.client/src/main.ts",
            "tsConfig": "packages/zauth.angular.client/tsconfig.prod.json",
            "assets": [],
            "styles": [
              "packages/zauth.angular.client/src/styles.scss"
            ],
            "scripts": [],
            "optimization": true,
            "outputHashing": "all",
            "sourceMap": false,
            "extractCss": true,
            "namedChunks": false,
            "aot": true,
            "extractLicenses": true,
            "vendorChunk": false,
            "buildOptimizer": true
          },
          "configurations": {
            "debug": {
              "tsConfig": "packages/zauth.angular.client/tsconfig.json",
              "optimization": false,
              "sourceMap": true,
              "buildOptimizer": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "zauth.angular.client:build:debug"
          }
        }
      }
    },
    "zauth.angular": {
      "root": "packages/zauth.angular",
      "sourceRoot": "src",
      "projectType": "library",
      "prefix": "z",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss",
          "spec": false
        },
        "@schematics/angular:class": {
          "spec": false
        },
        "@schematics/angular:directive": {
          "spec": false
        },
        "@schematics/angular:guard": {
          "spec": false
        },
        "@schematics/angular:module": {
          "spec": false
        },
        "@schematics/angular:pipe": {
          "spec": false
        },
        "@schematics/angular:service": {
          "spec": false
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "packages/zauth.angular/tsconfig.prod.json",
            "project": "packages/zauth.angular/ng-package.json"
          },
          "configurations": {
            "debug": {
              "tsConfig": "packages/zauth.angular/tsconfig.json"
            }
          }
        }
      }
    }
  },
  "defaultProject": "zauth.angular.client"
}
