{
  "name": "@zthun/works",
  "version": "0.0.0",
  "description": "The main website for zthunworks projects.",
  "author": "Anthony Bonta",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/zthun/works"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "rimraf 'dist' 'docs' 'reports' '*.tgz' '*.log' 'packages/**/dist' 'packages/**/docs' 'packages/**/*.tgz' 'packages/**/*.log'",
    "test": "vitest run --coverage",
    "e2e": "cucumber-js",
    "lint": "lint-janitor",
    "build": "lerna run build",
    "doc": "lerna run doc",
    "make": "yarn clean && yarn lint && yarn test && yarn build && yarn doc",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@cucumber/cucumber": "^9.4.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jsdom": "^21.1.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.197",
    "@types/md5": "^2.3.2",
    "@types/node": "^20.4.10",
    "@types/nodemailer": "^6.4.9",
    "@types/url-parse": "^1.4.8",
    "@types/uuid": "^9.0.2",
    "@vitest/coverage-istanbul": "^0.34.1",
    "@zthun/lint-janitor": "^14.2.0",
    "@zthun/lint-janitor-config": "^14.2.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "jsdom-global": "^3.0.2",
    "lerna": "^7.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.1"
  },
  "packageManager": "yarn@3.3.1"
}
