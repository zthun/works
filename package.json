{
  "name": "@zthun/works",
  "version": "0.0.0",
  "description": "The main website for zthunworks projects.",
  "author": "Anthony Bonta",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/zthun/works"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "rimraf 'dist' 'docs' 'reports' '*.tgz' '*.log' 'packages/**/dist' 'packages/**/docs' 'packages/**/*.tgz' 'packages/**/*.log'",
    "test": "jest --coverage",
    "e2e": "cucumber-js",
    "lint": "lint-janitor",
    "build": "lerna run build",
    "doc": "lerna run doc",
    "make": "yarn clean && yarn lint && yarn test && yarn build && yarn doc",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@cucumber/cucumber": "^8.10.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/jsdom": "^20.0.1",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/lodash": "^4.14.191",
    "@types/md5": "^2.3.2",
    "@types/node": "^18.11.18",
    "@types/nodemailer": "^6.4.7",
    "@types/selenium-webdriver": "^4.1.10",
    "@types/url-parse": "^1.4.8",
    "@types/uuid": "^9.0.0",
    "@zthun/lint-janitor": "^13.0.2",
    "@zthun/lint-janitor-config": "^13.0.2",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-junit": "^15.0.0",
    "jsdom": "^20.0.3",
    "lerna": "^6.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  }
}
