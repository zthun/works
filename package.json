{
  "name": "@zthun/works",
  "version": "0.0.0",
  "description": "The main website for zthunworks projects.",
  "author": "Anthony Bonta",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/zthun/works"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "rimraf --glob 'dist' 'docs' 'reports' '*.tgz' '*.log' 'packages/**/dist' 'packages/**/docs' 'packages/**/*.tgz' 'packages/**/*.log'",
    "test": "vitest run --coverage",
    "e2e": "cucumber-js",
    "lint": "lint-janitor",
    "build": "lerna run build",
    "make": "yarn clean && yarn lint && yarn test && yarn build",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@cucumber/cucumber": "^10.0.1",
    "@types/express": "^4.17.21",
    "@types/jsdom": "^21.1.6",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.0",
    "@types/supertest": "^2.0.16",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@zthun/lint-janitor": "^15.1.1",
    "@zthun/lint-janitor-config": "^15.1.1",
    "husky": "^8.0.3",
    "jsdom": "^23.0.0",
    "jsdom-global": "^3.0.2",
    "lerna": "^8.0.0",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "resolutions": {
    "rollup": "npm:@rollup/wasm-node@*"
  },
  "packageManager": "yarn@4.0.2"
}
