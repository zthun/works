{
  "name": "@zthun/works.api",
  "version": "3.0.6",
  "description": "This is an empty application used for mocking applications in other apps.",
  "author": "Anthony Bonta",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zthun/works",
    "directory": "packages/works.api"
  },
  "main": "dist/main.js",
  "bin": {
    "zthun-works-api": "./bin/main.js"
  },
  "engines": {
    "node": "^16.6.0"
  },
  "scripts": {
    "docker:publish": "docker push zthun/works.api:latest && docker push zthun/works.api:$(node -p \"require('./package.json').version\")",
    "docker:build": "docker build . -t zthun/works.api:latest -t zthun/works.api:$(node -p \"require('./package.json').version\")",
    "build": "tsc -p tsconfig.prod.json && npm pack",
    "debug": "nodemon",
    "start": "node ./dist/main.js"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.5",
    "@nestjs/core": "^8.2.5",
    "@nestjs/microservices": "^8.2.5",
    "@nestjs/platform-express": "^8.2.5",
    "@zthun/works.class": "^3.0.2",
    "@zthun/works.core": "^3.0.2",
    "@zthun/works.dal": "^3.0.6",
    "@zthun/works.nest": "^3.0.6",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.2",
    "uuid": "^8.3.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "bin"
  ]
}
