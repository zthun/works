{
  "name": "@zthun/works.security",
  "version": "1.0.0-15",
  "description": "The security library for @zthun scoped projects.  Contains hashing algorithms, cryptography, and token generation.",
  "author": "Anthony Bonta",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zthun/works"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "docs": "typedoc",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build": "yarn build:cjs && yarn build:esm"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**",
    "docs/**"
  ],
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "md5": "^2.3.0",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/md5": "^2.2.0",
    "@types/uuid": "^8.0.0"
  },
  "sideEffects": false
}
