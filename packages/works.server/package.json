  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zthun/works"
  },
  "main": "dist/main.js",
  "bin": {
    "zthun-works-server": "./dist/main.js"
  },
  "scripts": {
    "docker:publish": "docker push zthun/works.server:latest && docker push zthun/works.server:$(node -p \"require('./package.json').version\")",
