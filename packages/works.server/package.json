    "@zthun/works.core": "^1.0.0-22",
    "@zthun/works.dal": "^1.0.0-22",
    "@zthun/works.nest": "^1.0.0-22",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.3.1",
    "class-validator": "~0.13.1",
    "cookie-parser": "^1.4.5",
    "helmet": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongodb": "^3.6.2",
    "mongodb-memory-server": "^6.9.2",
    "nodemailer": "^6.4.10",
