{
  "name": "@zthun/works.themes",
  "version": "1.4.0",
  "description": "The core package for styles and themes for zthunworks.",
  "author": "Anthony Bonta",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zthun/works",
    "directory": "packages/works.themes"
  },
  "styles": "css/index.min.css",
  "css": "css/index.min.css",
  "less": "less/index.less",
  "scripts": {
    "build:mincss": "cleancss -o css/index.min.css css/index.css",
    "build:postcss": "postcss -o css/index.css css/index.css",
