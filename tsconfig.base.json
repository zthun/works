{
  "compilerOptions": {
    "module": "esnext",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "jsx": "react",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "declaration": true,
    "target": "es5",
    "sourceMap": true,
    "lib": [
      "es2017",
      "dom"
    ]
  }
}
